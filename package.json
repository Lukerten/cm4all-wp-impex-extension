{
  "name": "cm4all-wp-impex-extension",
  "version": "1.0.0",
  "description": "Plugin Extension for the cm4all-wp-impex Plugin. Import entire Elementor Sites to the Gutenberg Editor ",
  "main": "build/index.js",
  "scripts": {
    "wp-env": "scripts/wp-env/wp-env.sh",
    "wp-env:setup": "scripts/wp-env/wp-env-setup.sh",
    "wp-env:cleanup": "scripts/wp-env/wp-env-cleanup.sh",
    "test": "node --test",
    "test:watcher": "while true; do inotifywait -r -e modify . && node --test; done"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": true,
    "arrowParens": "always",
    "bracketSpacing": false
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended-scss",
      "stylelint-config-prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lukerten/cm4all-wp-impex-extension.git"
  },
  "author": "Luke Brendgen",
  "license": "Unlicense",
  "dependencies": {
    "@wordpress/env": "^4.9.0",
    "@wordpress/scripts": "^23.4.0",
    "classnames": "^2.3.1",
    "debug": "^4.3.4"
  },
  "peerDependencies": {
    "@wordpress/api-fetch": "^3.23.0",
    "@wordpress/block-editor": "^5.3.0",
    "@wordpress/block-library": "^2.29.0",
    "@wordpress/blocks": "^8.0.0",
    "@wordpress/components": "^13.0.0",
    "@wordpress/compose": "^3.25.0",
    "@wordpress/core-data": "^2.26.0",
    "@wordpress/data": "^4.27.0",
    "@wordpress/dom-ready": "^2.13.0",
    "@wordpress/edit-post": "3.27.0",
    "@wordpress/editor": "^9.26.0",
    "@wordpress/element": "^2.20.0",
    "@wordpress/i18n": "^3.19.0",
    "@wordpress/keyboard-shortcuts": "^3.0.3",
    "@wordpress/plugins": "2.25.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "uuid": "^8.3.2"
  }
}